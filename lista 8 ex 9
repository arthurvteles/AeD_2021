#include <stdio.h>
#include <stdlib.h>
#include <math.h>



int fibonacci (int n) ;

int main (){

    int n ; 

    printf("\nDigite quantos elementos desejas : ") ; 
        scanf("%d",&n ); 

    printf("\nFibonacci de n : %d",fibonacci(n)); 


    return 0 ; 
}


int fibonacci(int n){

    if((n == 0) || (n == 1)){
        return n ;
    }
    else{
        return fibonacci(n-1) + fibonacci (n-2) ; 
    }
    



}
